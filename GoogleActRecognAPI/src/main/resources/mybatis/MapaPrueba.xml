<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.acerodocs.api.db.dao.PostgreSQLService">

	<!-- ************** READ ************** -->
	<select id="getPruebas" resultType="com.googleActRecognDataServer.api.postgres.pojos.Prueba">
		SELECT
		*
		FROM "tPrueba";
	</select>

	<select id="getUnaPrueba" resultType="com.googleActRecognDataServer.api.postgres.pojos.Prueba">
		SELECT
		*
		FROM "tPrueba"
		WHERE nombre=#{nombre};
	</select>

</mapper>